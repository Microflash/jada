{
  "name": "Jada",
  "description": "A versatile blog starter for Gridsome",
  "url": "https://gridsome-jada.netlify.com",
  "favicon": "https://raw.githubusercontent.com/Microflash/jada/master/src/favicon.png",
  "maintainer": "Jada Zorebian",
  "tocPattern": "Table of Contents",
  "searchConfig": {
    "file": {
      "dir": "static",
      "name": "search.json"
    },
    "options": {
      "shouldSort": true,
      "includeMatches": true,
      "tokenize": true,
      "matchAllTokens": true,
      "threshold": 0.4,
      "location": 0,
      "distance": 600,
      "maxPatternLength": 32,
      "minMatchCharLength": 1,
      "keys": ["title", "blurb"]
    }
  }
}
